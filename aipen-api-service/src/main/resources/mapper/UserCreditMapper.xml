<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eningqu.mapper.UserCreditMapper">

    <select id="selectUserCreditByUid" resultType="com.eningqu.domain.api.UserCredit">
        SELECT id,uid,vip_level,vip_credit FROM nq_user_vip WHERE uid = #{uid}
    </select>

</mapper>
